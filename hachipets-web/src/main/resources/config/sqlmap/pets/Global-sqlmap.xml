<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="GlobalConfig">

	<select id="get" resultClass="java.lang.String" parameterClass="map">
		SELECT config
		FROM Pets_GlobalConfig
		WHERE name = #name#
	</select>
	
</sqlMap>