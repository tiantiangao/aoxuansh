<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PetsMovieRecommend">

    <select id="findRecommendMovieID" resultClass="java.lang.Integer" parameterClass="map">
        SELECT movieId
        FROM Pets_Movie_Recommend
        ORDER BY addTime DESC
        limit 1
    </select>

</sqlMap>