<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PetsMovieRegion">

    <select id="findMovieRegionList" resultClass="java.lang.String" parameterClass="map">
        <![CDATA[
		SELECT `region`
		FROM Pets_Movie_Region
		ORDER BY ID
        ]]>
    </select>
    
    <select id="findMovieRegionById" resultClass="java.lang.String" parameterClass="map">
        <![CDATA[
		SELECT `region`
		FROM Pets_Movie_Region
		WHERE ID=#id#
        ]]>
    </select>

</sqlMap>